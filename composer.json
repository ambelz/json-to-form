{
    "name": "ambelz/json-to-form",
    "description": "Symfony bundle to transform JSON structures into forms",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Christophe Abillama",
            "email": "christophe.abillama@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/form": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/ux-dropzone": "^2.27",
        "symfony/ux-live-component": "^2.27",
        "symfony/validator": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ambelz\\JsonToFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ambelz\\JsonToFormBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    }
}
